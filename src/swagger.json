  "paths":{
    "/categories":{
      "post":{
        "tags": ["Category"],
        "sumary": "Create an category",
        "description": "Create a new category",
        "requestBody":{
          "content":{
            "application/json":{
              "schema": {
                "type": "object",
                "properties":{
                  "name": {
                    "type": "string"
                  },
                  "description":{
                    "type": "string"
                  }
                },
                "example":{
                  "name": "Category name sample",
                  "description": "Category description sample"
                }
              }
            }
          }
        },
        "responses":{
          "201": {
            "description": "Created"
          },
          "500":{
            "description": "Category Already Exists"
          }
        }
      },
      "get":{
        "tags":["Category"],
        "sumary": "List categories",
        "description": "List all categories",
        "responses":{
          "200":{
            "description":"Categories"
          }
        }
      }
    }
  }
